<div data-track="{{gameID}}" class="my-5">
  <div class="d-flex justify-content-center">
    <button data-ready="" type="button" class="btn btn-success">Ready to fight</button>

  </div>
  <div class="d-flex py-4 justify-content-center">
    <div data-char="" class="charContainer"></div>
  </div>

  <div data-paths>
    {{#each players}}
    <div data-player="{{this.id}}" class="path {{#if this.ready}} ready {{/if}}">
      <div class="section active ">
        <div data-name="{{this.nick}}" class="car">

          {{this.skin}}
        </div>
      </div>

      {{#times ../gameConfig.trackLength}}
      <div class="section"></div>
      {{/times}}

    </div>
    {{/each}}
  </div>

</div>

